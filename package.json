{
    "name": "n8n-nodes-itop",
    "version": "1.0.2",
    "description": "n8n community node for iTop CMDB integration",
    "license": "MIT",
    "homepage": "https://github.com/itomig-de/n8n-nodes-itop#readme",
    "keywords": [
        "n8n-community-node-package",
        "itop",
        "cmdb",
        "itsm"
    ],
    "author": {
        "email": "lars.kaltefleiter@itomig.de",
        "name": "Lars Kaltefleiter"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/itomig-de/n8n-nodes-itop.git"
    },
    "bugs": {
        "url": "https://github.com/itomig-de/n8n-nodes-itop/issues"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "release": "n8n-node release",
        "prepublishOnly": "n8n-node prerelease"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "strict": true,
        "credentials": [
            "dist/credentials/ITopApi.credentials.js",
            "dist/credentials/ITopTokenApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/ITop/ITop.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    },
    "publishConfig": {
        "access": "public"
    }
}
